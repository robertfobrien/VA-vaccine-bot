          posts,\n            }\n        })\n        console.log(postStats);\n        console.log(facebookStats);\n}\n\n        await browser.close();\n\n        })();\n\n\n    res.status(200).json({ info: 'scrape page', name: req.body.name })\n  }\n  "]},"metadata":{},"sourceType":"module"}