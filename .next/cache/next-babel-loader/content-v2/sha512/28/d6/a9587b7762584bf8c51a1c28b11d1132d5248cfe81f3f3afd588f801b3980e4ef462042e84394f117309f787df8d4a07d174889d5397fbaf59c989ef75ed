     waitUntil: 'networkidle2'\n      });\n      let videoStats = await page.evaluate(() => {\n        var name = document.querySelector('#channel-name > div > div').innerText;\n        var videos = document.querySelectorAll('#items')[4].innerHTML;\n        var type = 'youtube-videos';\n        return {\n          type,\n          name,\n          videos\n        };\n      });\n      console.log(youtubeStats);\n      console.log(videoStats);\n    }\n\n    await browser.close();\n  })();\n\n  res.status(200).json({\n    info: 'scrape page',\n    name: req.body.name\n  });\n});","map":{"version":3,"sources":["/Users/robertobrien/Documents/projects/ghost-scraper/pages/api/scrapePage.js"],"names":["req","res","puppeteer","require","browser","launch","headless","page","newPage","body","youtubelink","goto","waitUntil","youtubeStats","evaluate","name","document","querySelector","innerText","description","subscriberString","slice","length","endLetter","subscriberPrefix","subscriberNum","parseInt","parseFloat","viewsString","querySelectorAll","replace","type","viewsNum","videoStats","videos","innerHTML","console","log","close","status","json","info"],"mappings":"AACA,gBAAe,CAACA,GAAD,EAAMC,GAAN,KAAc;AAEzB,QAAMC,SAAS,GAAGC,OAAO,CAAC,WAAD,CAAzB;;AAEA,GAAC,YAAY;AACT,QAAIC,OAAO,GAAG,MAAMF,SAAS,CAACG,MAAV,CAAiB;AAACC,MAAAA,QAAQ,EAAE;AAAX,KAAjB,CAApB;AACA,QAAIC,IAAI,GAAG,MAAMH,OAAO,CAACI,OAAR,EAAjB;;AAEA,QAAGR,GAAG,CAACS,IAAJ,CAASC,WAAT,IAAwB,EAA3B,EACA;AACA,YAAMH,IAAI,CAACI,IAAL,CAAUX,GAAG,CAACS,IAAJ,CAASC,WAAT,GAAuB,QAAjC,EAA2C;AAACE,QAAAA,SAAS,EAAE;AAAZ,OAA3C,CAAN;AAEA,UAAIC,YAAY,GAAG,MAAMN,IAAI,CAACO,QAAL,CAAc,MAAM;AAEzC,YAAIC,IAAI,GAAGC,QAAQ,CAACC,aAAT,CAAuB,2BAAvB,EAAoDC,SAA/D;AACA,YAAIC,WAAW,GAAGH,QAAQ,CAACC,aAAT,CAAuB,cAAvB,EAAuCC,SAAzD;AACA,YAAIE,gBAAgB,GAAGJ,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,EAA4CC,SAAnE;AACAE,QAAAA,gBAAgB,GAAGA,gBAAgB,CAACC,KAAjB,CAAuB,CAAvB,EAA0BD,gBAAgB,CAACE,MAAjB,GAAwB,EAAlD,CAAnB;AAEA,YAAIC,SAAS,GAAGP,QAAQ,CAACC,aAAT,CAAuB,mBAAvB,EAA4CC,SAA5C,CAAsDG,KAA